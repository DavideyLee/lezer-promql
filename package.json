{
  "name": "lezer-promql",
  "version": "0.7.0",
  "description": "lezer-based PromQL grammar",
  "main": "index.cjs",
  "type": "module",
  "exports": {
    "import": "index.es.js",
    "require": "index.cjs"
  },
  "module": "index.es.js",
  "types": "index.d.ts",
  "author": "Julius Volz <julius.volz@promlabs.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "lezer-generator": "^0.10.1",
    "mocha": "^8.0.1",
    "rollup": "^2.21.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "dependencies": {
    "lezer": "^0.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/promlabs/lezer-promql.git"
  },
  "scripts": {
    "build": "lezer-generator src/promql.grammar -o src/parser && cat src/parser.terms.js >> src/parser.js && cat src/promql.terms.js >> src/parser.js && rollup -c",
    "build-debug": "lezer-generator src/promql.grammar --names -o src/parser && cat src/promql.terms.js >> src/parser.js && rollup -c",
    "test": "mocha test/test-*.js"
  },
  "keywords": [
    "lezer",
    "promql"
  ],
  "bugs": {
    "url": "https://github.com/promlabs/lezer-promql/issues"
  },
  "homepage": "https://github.com/promlabs/lezer-promql#readme"
}
